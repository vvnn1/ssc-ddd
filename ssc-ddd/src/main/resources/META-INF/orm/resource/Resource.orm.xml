<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm http://xmlns.jcp.org/xml/ns/persistence/orm_2_2.xsd"
                 version="2.2">
    <entity class="indi.melon.ssc.resource.domain.resource.Resource" access="FIELD">
        <attributes>
            <embedded-id name="id" access="FIELD">
                <attribute-override name="value">
                    <column name="id"/>
                </attribute-override>
            </embedded-id>
            <basic name="name"/>
            <basic name="createTime"/>
            <basic name="updateTime"/>
            <basic name="refCount"/>
            <embedded name="fileID">
                <attribute-override name="value">
                    <column name="fileID"/>
                </attribute-override>
            </embedded>
        </attributes>
    </entity>
</entity-mappings>